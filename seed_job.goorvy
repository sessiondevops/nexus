def branch_name = ${env.Brach_Name}
job('${branch_name}_example') {
    scm {
        github('https://github.com/sessiondevops/nexus.git', 'development')
    }
    def releaseScript = readFileFromWorkspace('Jenkinsfile')
    steps {
        groovyCommand(releaseScript)
    }
    triggers {
        scm('*/1 * * * *')
    }
}
